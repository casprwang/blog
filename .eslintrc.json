{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": "airbnb",
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {},
	"settings": {
		"react": {
			"version": "16.8.2"
		},
		"import/resolver": {
			"node": {
				"paths": ["src"],
				"extensions": [".js", ".jsx"]
			}
		}
	}
}
